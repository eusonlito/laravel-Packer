{
    "name": "laravel/packer",
    "keywords": ["packer", "minify", "laravel"],
    "description": "A package for pack css and javascript files",
    "license": "MIT",
    "authors": [
        {
            "name": "Lito",
            "email": "lito@eordes.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "imagecow/imagecow": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "mikey179/vfsStream": "*"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Packer\\": "src/Laravel/Packer/"
        },
        "classmap": [
            "src/Laravel/Packer/Processors/"
        ],
        "files": [
                "src/Laravel/Packer/Exceptions.php"
        ]
    },
    "minimum-stability": "stable"
}
